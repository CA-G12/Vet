{
    "root":true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint-config-react-typescript",
        "plugin:@typescript-eslint/recommended",
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "project": "tsconfig.json",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "@typescript-eslint/explicit-function-return-type": "warn"
    }
}
